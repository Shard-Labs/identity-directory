{
  "name": "identity-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "deps:start": "docker-compose           up -d --remove-orphans",
    "deps:pause": "docker-compose           stop",
    "deps:close": "docker-compose           down --remove-orphans",
    "deps:purge": "npm -s run deps:close -- -v",
    "deps:reset": "npm -s run deps:purge; npm -s run deps:start; sleep 4"
  },
  "dependencies": {
    "@polkadot/api": "^6.3.1",
    "@polkadot/extension-dapp": "^0.40.3",
    "@polkadot/keyring": "^7.4.1",
    "@polkadot/util": "^7.4.1",
    "@popperjs/core": "2.9.2",
    "@tailwindcss/postcss7-compat": "npm:@tailwindcss/postcss7-compat@^2.1.0",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "blueimp-md5": "^2.18.0",
    "core-js": "^3.10.2",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.0",
    "v-click-outside": "^3.1.2",
    "vue": "3.0.11",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.6",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/blueimp-md5": "^2.18.0",
    "@types/chai": "^4.2.16",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-router": "^4.5.12",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-plugin-unit-mocha": "^4.5.13",
    "@vue/cli-plugin-vuex": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.2.2",
    "autoprefixer": "^9.8.6",
    "chai": "^4.3.4",
